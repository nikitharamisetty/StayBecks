  <body style="font-family: Times New Roman; background-color: rgb(146, 132, 104)">

    <div class="container">
      <div class="row">
        <div class="card mt-4 bg-dark">
          <div class="card-body">
            <form>
              <div class="row">
                <div class="input-group col">
                  <span class="input-group-text"> <i class="bi bi-geo-alt"></i> </span>
                
                  <input type="text" class="form-control" placeholder="Search Destination " name ="searchname">
                  
                </div>
                <div class="input-group col">
                  <span class="input-group-text date">  <svg data-id="SVG_CHECK_IN__16" focusable="false" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="none" viewBox="0 0 16 16"><path stroke="currentColor" d="M1.703 8.565h3.288V5.277M.806 4.379l4.186 4.187"></path><path stroke="currentColor" d="M1.703 10.67v2.45a1.5 1.5 0 001.5 1.5h11.225V4.01a1.5 1.5 0 00-1.5-1.5H1.703"></path><path fill="currentColor" stroke="none" fill-rule="evenodd" d="M10.366 9.056h1.892V7.164h-1.892v1.892zM10.366 12.563h1.892V10.67h-1.892v1.893zM7.119 12.563h1.893V10.67H7.119v1.893zM3.874 12.563h1.892V10.67H3.874v1.893z" clip-rule="evenodd"></path><path fill="currentColor" stroke="none" fill-rule="evenodd" d="M10.366 9.056h1.892V7.164h-1.892v1.892zM7.119 9.056h1.893V7.164H7.119v1.892z" clip-rule="evenodd"></path><path stroke="currentColor" d="M4.41.998v1.513M11.2.998v1.513M14.407 5.277H7.119"></path></svg>  </span>
                  <input type="text" onfocus="(this.type='date')" onblur="(this.type='text')" class="form-control" placeholder="Check-In">
                </div>
                <div class="input-group col">
                  <span class="input-group-text date"> <svg data-id="SVG_CHECK_OUT__16" focusable="false" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="none" viewBox="0 0 16 16"><path stroke="currentColor" d="M11.104 5.09h3.289v3.286M10.206 9.275l4.187-4.186"></path><path stroke="currentColor" d="M14.331 10.544v3.95H2.742a1.136 1.136 0 01-1.135-1.136V2.385h12.724"></path><path fill="currentColor" stroke="none" fill-rule="evenodd" d="M5.668 8.93H3.776V7.036h1.892V8.93zM5.668 12.437H3.776v-1.893h1.892v1.893zM8.915 12.437H7.022v-1.893h1.893v1.893zM12.16 12.437h-1.892v-1.893h1.892v1.893zM8.915 8.93H7.022V7.036h1.893V8.93z" clip-rule="evenodd"></path><path stroke="currentColor" d="M11.623.872v1.513M4.834.872v1.513M1.628 5.15h7.287"></path></svg> </span>
                  <input type="text" onfocus="(this.type='date')" onblur="(this.type='text')" class="form-control" placeholder="Check-Out">
                </div>
                <div class="d-grid col-md-2">
                  <button type="submit" class="btn btn-primary"> <i class="bi bi-search"></i>   Search </button>
                
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>


    <div class="container-fluid">
      <div class="row mt-2 pt-5">
        <div class="col-md-3">
          <div class="trial">
            <div class="link text">
              My Bookings</div>
            <div class="link text">
              Property Listings</div>
            <div class="link text" ><a href="/fav">
              Favorites</a></div>
            <div class="link text">
              Customer Care</div>
            <div class="link text">
              About Us</div>
            <div class="link text">
              Help</div>
          </div>
        </div>
      <div class="col-md-9">
        <h2 class="text-dark"> All properties in United States </h2>  
      <div class="row row-cols-1 row-cols-md-4 ">
 <% properties.forEach(function(property){ %> 
              <div class="col-sm-3">
                <div class="card h-100 border-dark">

                  <img src="/images/<%= property.image %>" class="card-img-top" alt="property_1">
                  <div class="card-body d-flex flex-column">
                                     
                    <p> <%=property.title%></p>
                   
                    <p> Price : <%=property.price_per_night%></p>
                    <p> Rating : <%=property.rating%></p><br>                 
                    <div class="d-grid">
                    <button type="submit" class="btn btn-dark mt-auto"> <i class="bi bi-house-fill"></i> Book Now </button>
                    </div>
              
                  </div>
                  <div class="card-footer">
                    <div class="d-grid">
                    <button type="submit" class="btn btn-pink">  <a href="/properties/<%= property._id %>" class="btn btn-primary">Show details</a></button>
                    <button type="submit" class="btn btn-pink">  <a href="/fav/Delete/<%= property._id %>" class="btn btn-primary">Delete from Favourite</a></button>
                    
                    </div>
                  </div>
                </div>
              </div>
              <% }); %>  
      </div>
    </div>
      </div> 
 </div>

