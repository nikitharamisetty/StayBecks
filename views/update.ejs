  <body style="font-family: Times New Roman; background-color: rgb(146, 132, 104)">

    <div class="container" style="width: 500px">
        <h3> Let's create your account</h3>
      <div class="row">
        <div class="card mt-4 bg-dark">
          <div class="card-body">
        <form method="post" action="/properties/<%=property._id%>/update?_method=PUT">
            <div class="form-group">
            <label>Title</label>
            <input type="text" id="title" name="title" class="form-control" value="<%=property.title%>">
            </div>
            <div class="form-group">
            <label>Location</label>
            <input type="text" id="location" name="location" class="form-control" value="<%=property.location%>">
            </div>
            <div class="form-group">
            <label>Bedrooms</label>
            <input type="text" id="bedrooms" name="bedrooms" class="form-control" value="<%=property.bedrooms%>">
            </div>
            <div class="form-group ">
            <label>Amenities</label>
            <input type="text" id="amenities" name="amenities" class="form-control" value="<%=property.Amenities%>">
            </div>
            <div class="form-group ">
            <label>Price per night</label>
            <input type="text" id="price" name="price" class="form-control" value="<%=property.price_per_night%>">
            </div>
            <div class="form-group ">
            <label>Nightly Fee</label>
            <input type="text" id="nightlyfee" name="nightlyfee" class="form-control" value="<%=property.nightly_fee%>">
            </div>
            <div class="form-group ">
            <label>Cleaning Fee</label>
            <input type="text" id="cleaningfee" name="cleaningfee" class="form-control" value="<%=property.cleaning_fee%>">
            </div>
            <div class="form-group ">
            <label>Service Fee</label>
            <input type="text" id="servicefee" name="servicefee" class="form-control" value="<%=property.service_fee%>">
            </div>
            <div class="form-group ">
            <label>Rating</label>
            <input type="text" id="rating" name="rating" class="form-control" value="<%=property.rating%>">
            </div>
            <div class="form-group ">
                <label>Image name</label>
                <input type="text" id="image" name="image" class="form-control" value="<%=property.image%>">
                </div>
        
            <div class="form-group ">
            <label>Description</label>
            <textarea id="desc" name="desc" class="form-control" value="<%=property.description%>">
            </textarea>
            </div>
            <div class="form-group ">
            <button type="submit" id="add" class="btn btn-primary">Update Listing</button>
            </div>
        
        </form>
        
          </div>
        </div>
      </div>
    </div>



      
